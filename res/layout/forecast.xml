			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:id="@+id/forecast_table_short"
			android:shrinkColumns="*">
			 
			<TableRow>
				<com.SyntheticCode.BlueSkyWeather.ForecastShortView
					android:id="@+id/forecast_short_row0" />
			</TableRow>
			<TableRow>
				<com.SyntheticCode.BlueSkyWeather.ForecastShortView
					android:id="@+id/forecast_short_row1" />
			</TableRow>
			<TableRow>
				<com.SyntheticCode.BlueSkyWeather.ForecastShortView
